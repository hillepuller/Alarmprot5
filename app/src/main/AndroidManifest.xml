<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alarmprot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MySettings"
            android:label="@string/title_activity_my_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PersonalSettings"
            android:label="@string/title_activity_personal_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PopUp"
            android:label="@string/title_activity_pop_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PreTravelSettings"
            android:label="@string/title_activity_pre_travel_settings"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
